 #include <stdio.h>
main(){   int i,j;   for (i = 97; i <= 122; i++)   {      for (j = 122; i <= j; j--)      {         printf(" ");}      for (j = i; j <= i; j++)      {         printf("*", j);}      for (j = 2; j <= 2 * (i - 96) - 2; j++)      {         printf(" ");
}      for (j = i; j <= i && j > 97; j++)      {         printf("*", j);      }      printf("\n");   }
}
